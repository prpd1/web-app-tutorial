pipeline {
    agent any
    stages {
        stage('Build image') {
            steps {
                echo 'Starting to build docker image'

                script {
                    def customImage = docker.build('qwerty703/qwerty',"./docker")
                    docker.withRegistry('https://registry.hub.docker.com', 'qwerty703') {
                    customImage.push("${env.BUILD_NUMBER}")
                }
            }
        }
    }
}
}
